  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Button', '1').ClickButton;
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Button', '3').ClickButton;
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Button', '=').ClickButton;
  aqObject.CompareProperty(Sys.Process('calc').Window('SciCalc', 'Calculadora', 1).Window('Edit', '', 1).wText, cmpMatches, '12', false);
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Button', 'C').ClickButton;
  }
end;

procedure Test1;
begin
  Aliases.Microsoft_WindowsCalculator.Calculadora.LandmarkTarget.Controles_de_exibi_o.Limpar.Keys('1');
  Aliases.Microsoft_WindowsCalculator.Calculadora.LandmarkTarget.Controles_de_exibi_o.Limpar.Keys('0[NumPlus]10[Enter][Esc]');

  {
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Keys('1');
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Edit').Keys('0[NumPlus]10[Enter][Esc]');
  }
end;

procedure Test2;
begin
  Aliases.Microsoft_WindowsCalculator.Calculadora.LandmarkTarget.Controles_de_exibi_o.Limpar.Keys('7');
  Aliases.Microsoft_WindowsCalculator.Calculadora.LandmarkTarget.Controles_de_exibi_o.Limpar.Keys('+9[Enter]');
  Aliases.Microsoft_WindowsCalculator.Calculadora.LandmarkTarget.Controles_de_exibi_o.Limpar.Click; 

  {
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Keys('7');
  Sys.Process('calc').Window('SciCalc', 'Calculadora').text:= '7';
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Edit').Keys('+9[Enter]');
  Sys.Process('calc').Window('SciCalc', 'Calculadora').Window('Button', 'C').ClickButton;
  }
end;

